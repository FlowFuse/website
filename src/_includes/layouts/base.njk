---
meta:
  language: en-US
  type: page
  site:
    name: FlowFuse
    description: Bespoke, flexible, and resilient manufacturing applications
    url: https://FlowFuse.com
    logo:
      src: https://flowfuse.com/handbook/images/logos/ff-logo--square--dark.png
      width: 396
      height: 215
  author:
    name: FlowFuse
  image:
    src: https://flowfuse.com/handbook/images/logos/ff-logo--square--dark.png
eleventyComputed:
  meta:
    url: "{{ page.url | url | toAbsoluteUrl }}"
    title: "{{ title }}"
    description: "{{ description }}"
    modified: "{{ page.date }}"
