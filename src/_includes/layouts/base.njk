<script>
    const getCookieValue = (name) => (
        document.cookie.match('(^|;)\\s*' + name + '\\s*=\\s*([^;]+)')?.pop() || ''
    )

    function decodeCookie (cookie) {
        return JSON.parse(decodeURIComponent(cookie))
    }

    var phBootstrap = {}

    if (!!(getCookieValue("ff-distinctid"))) {
        phBootstrap['distinctID'] = getCookieValue("ff-distinctid")
    }

    if (!!(getCookieValue("ff-feats"))) {
        phBootstrap['featureFlags'] = decodeCookie(getCookieValue("ff-feats"))
    } else {
        console.log("No feature flags found - falling back to 'control' content")
    }

