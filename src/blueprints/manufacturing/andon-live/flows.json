        "name": "Set Shift Label",
        "rules": [
            {
                "t": "set",
                "p": "payload",
                "pt": "msg",
                "to": "(\t    /*\t    * JSONata function to get the current day name and append a time-based suffix.\t    *\t    * The suffix is determined by the hour:\t    * - \"Early\" if the hour is 6 AM or later (and before 2 PM)\t    * - \"Late\" if the hour is 2 PM or later (and before 10 PM)\t    * - \"Night\" if the hour is 10 PM or later\t    *\t    * @param {number} [timestamp] - Optional. A Unix timestamp (milliseconds) to use.\t    * If not provided, the current time ($now()) will be used.\t    * @returns {string} - The day name followed by the appropriate time suffix (e.g., \"Friday Early\").\t    */\t    $timestamp := $now();\t    $m := $moment($timestamp);\t\t    /* Get the day name (e.g., \"Monday\", \"Tuesday\") */\t    $dayName := $m.format('dddd');\t\t    /* Get the hour (0-23) */\t    $hour := $m.hour();\t\t    /* Determine the time suffix based on the hour */\t    $timeSuffix := (\t        $hour >= 22 ? \"Night\" : /* 10 PM (22:00) or later */\t        $hour >= 14 ? \"Late\" :  /* 2 PM (14:00) or later */\t        $hour >= 6 ? \"Early\" :  /* 6 AM (06:00) or later */\t        \"Night\"                 /* Before 6 AM (considered Night) */\t    );\t\t    /* Combine the day name and the time suffix */\t    $dayName & \" \" & $timeSuffix\t\t)",
                "tot": "jsonata"
            }
        ],
        "action": "",
        "property": "",
        "from": "",
        "to": "",
