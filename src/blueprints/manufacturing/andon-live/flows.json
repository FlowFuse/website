    {
        "id": "56b85be367b59317",
        "type": "function",
        "z": "bafca6bb93ab832b",
        "g": "a3f7447948120b78",
        "name": "seconds to minute:seconds",
        "func": "let secsDecimal = msg.payload;\n\nlet mins = Math.floor(secsDecimal / 60);\nlet secs = secsDecimal % 60;\n\n// Format the minutes and seconds into \"mm:ss\" format\nmsg.payload = (mins < 10 ? '0' : '') + mins + ':' + (secs < 10 ? '0' : '') + secs;\n\nreturn msg;",
        "outputs": 1,
        "timeout": "",
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
        "x": 1020,
        "y": 240,
        "wires": [
            []
        ]
    },
    {
        "id": "11c3bf981a8b75cb",
        "type": "function",
        "z": "bafca6bb93ab832b",
        "g": "a3f7447948120b78",
        "name": "toggle flow.clock_state",
        "func": "let clock_state = flow.get(\"clock_state\") || \"STOP\";\n\nclock_state = clock_state === \"RUN\" ? \"RUN\" : (clock_state || \"STOP\");\n\nflow.set(\"clock_state\", clock_state );\n\nmsg.payload = +(flow.get(\"clock_value\") || 0);\n\nif (isNaN(msg.payload)) {\n    msg.payload = 0\n    flow.set(\"clock_value\", 0)\n}\n\nreturn msg;",
        "outputs": 1,
        "timeout": "",
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
        "x": 720,
