    {
        "id": "4f71dedcce348bad",
        "type": "function",
        "z": "bafca6bb93ab832b",
        "g": "a3f7447948120b78",
        "name": "increment flow.clock_value",
        "func": "let clock_value = flow.get(\"clock_value\") || 0;\n\nclock_value++; \n\nif (clock_value < 0 || isNaN(clock_value)) clock_value = 0;\n\nflow.set(\"clock_value\", clock_value);\n\nmsg.payload = clock_value;\nreturn msg;",
        "outputs": 1,
        "timeout": "",
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
        "x": 700,
