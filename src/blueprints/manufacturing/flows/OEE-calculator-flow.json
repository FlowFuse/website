    {
        "id": "49a137d68125419b",
        "type": "function",
        "z": "893bc3c905890ddd",
        "name": "Transform to table",
        "func": "// Create an array to hold the table rows\nconst tableData = [];\n\n// Transform the KPIs into table rows\ntableData.push({\n    \"KPI\": \"OEE\",\n    \"Value\": msg.payload.OEE\n});\ntableData.push({\n    \"KPI\": \"Availability\",\n    \"Value\": msg.payload.Availability\n});\ntableData.push({\n    \"KPI\": \"Performance\",\n    \"Value\": msg.payload.Performance\n});\ntableData.push({\n    \"KPI\": \"Quality\",\n    \"Value\": msg.payload.Quality\n});\ntableData.push({\n    \"KPI\": \"Codes\",\n    \"Value\": msg.payload.scrapCode\n});\n\n// Output the table data in the payload\nmsg.payload = tableData;\n\nreturn msg;\n",
