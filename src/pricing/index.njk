<div class="contentDedicated hide w-full px-6" id="contact-us-dedicated">
    <div class="flex flex-col m-auto max-w-2xl mt-10 pb-12 gap-12">
        <h2 class="w-full text-center"><span class="text-indigo-600">Contact us</span> for pricing
        </h2>
        <div>
            {% hubspotForm hubspot.dedicated.formId, hubspot.dedicated.cta, hubspot.reference, 'dedicatedPricingForm' %}
        </div>
    </div>
</div>

<!-- Frequently Asked Questions -->
<div class="px-6 md:px-14 max-w-screen-lg mx-auto">
    {% include "faq.njk" %}
</div>
<!-- Scroll to anchor -->
{% include "smooth-scroll.njk" %}

<!-- Toggle -->
<script>
    // Get the hosting and billing parameters from the URL
    const urlParams = new URLSearchParams(window.location.search);
    let hosting = urlParams.get('hosting') || 'cloud';

    // Hosting toggle
    const btns = document.querySelectorAll(".mainToggle");
    const cloud = document.querySelectorAll(".contentCloud");
    const selfHosted = document.querySelectorAll(".contentSelfHosted");
    const dedicated = document.querySelectorAll(".contentDedicated"); // New dedicated elements
    const cloudText = document.querySelector(".cloudText");
    const selfHostedText = document.querySelector(".selfHostedText");
    const dedicatedText = document.querySelector(".dedicatedText");
    const selector = document.querySelector(".selector");
    const featureComparison = document.querySelector("#feature-comparison");

    function updateContentAndUrl(hosting) {
        cloudText.classList.remove('active');
        selfHostedText.classList.remove('active');
        dedicatedText.classList.remove('active');

        if (hosting === 'cloud') {
            selector.style.left = "0.8%";
            cloud.forEach((item) => item.classList.remove("hide"));
            selfHosted.forEach((item) => item.classList.add("hide"));
            dedicated.forEach((item) => item.classList.add("hide"));
            urlParams.delete('hosting');
            cloudText.classList.add('active');
            featureComparison.classList.remove("hide");
        } else if (hosting === 'self-hosted') {
            selector.style.left = "33%";
            cloud.forEach((item) => item.classList.add("hide"));
            selfHosted.forEach((item) => item.classList.remove("hide"));
            dedicated.forEach((item) => item.classList.add("hide"));
            urlParams.set('hosting', hosting);
            selfHostedText.classList.add('active');
            featureComparison.classList.remove("hide");
        } else {
            selector.style.left = "66.2%";
            cloud.forEach((item) => item.classList.add("hide"));
            selfHosted.forEach((item) => item.classList.add("hide"));
            dedicated.forEach((item) => item.classList.remove("hide"));
            urlParams.set('hosting', hosting);
            dedicatedText.classList.add('active');
            featureComparison.classList.add("hide");
        }

        // Update the history
        let paramString = urlParams.toString();
        let currentHash = window.location.hash; // Preserve the current hash
        window.history.replaceState({}, '', paramString ? "?" + paramString + currentHash : window.location.pathname + currentHash);
    }

    // Check the hosting parameter when the page loads
    updateContentAndUrl(hosting);

    cloudText.addEventListener("click", function() {
        if (hosting === 'cloud') return;
        hosting = 'cloud';
        updateContentAndUrl(hosting);
        capture('cloud');
    });

    selfHostedText.addEventListener("click", function() {
        if (hosting === 'self-hosted') return;
        hosting = 'self-hosted';
        updateContentAndUrl(hosting);
        capture('self-hosted');
    });

    dedicatedText.addEventListener("click", function() {
        if (hosting === 'dedicated') return;
        hosting = 'dedicated';
        updateContentAndUrl(hosting);
        capture('dedicated');
    });
</script>

<!-- Features info -->
<script>
    function openInfo(id, hosting) {
        capture('info-feature', { feature: id }, { hosting: hosting })
        document.getElementById("ff-dialog--feature--" + id + "-" + hosting).classList.add('active')
    }
    function closeInfo(id, hosting) {
        document.getElementById("ff-dialog--feature--" + id + "-" + hosting).classList.remove('active')
    }
</script>
